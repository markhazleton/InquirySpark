@{
    var currentYear = DateTime.Now.Year.ToString();
}
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="description" content="InquirySpark Admin - Mark Hazleton">
    <meta name="author" content="Mark Hazleton">
    <link rel="icon" href="~/favicon.ico" type="image/x-icon">

    <title>@ViewData["Title"] - InquirySpark Admin</title>

    <!-- Bundled Site CSS (includes Bootstrap, Bootstrap Icons, DataTables) -->
    <link rel="stylesheet" href="~/css/site.min.css" asp-append-version="true" />

    @RenderSection("HeadScripts", required: false)
</head>
<body id="page-top">
<header>
    @await Html.PartialAsync("/Views/Shared/_nav.cshtml")
</header>
<main role="main" class="content-container pb-3">
    <section class="container-fluid pt-4 mt-4 mb-4 pb-4">
        @RenderBody()
    </section>
</main>




    @await Html.PartialAsync("/Views/Shared/_Footer.cshtml")

    <!-- PDFMake Virtual File System (required for PDF fonts) -->
    <script src="~/js/vfs_fonts.js" asp-append-version="true"></script>

    <!-- Bundled Site JS (includes jQuery, Bootstrap, DataTables, and all dependencies) -->
    <script src="~/js/site.min.js" asp-append-version="true"></script>

    @RenderSection("Scripts", required: false)
</body>
</html>
